pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World, This is Julia Test'
            }
        }
        stage('Build') {
            steps {
                echo 'Building'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
        stage('Test') {
            steps {
                echo 'Test'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing the Product!!!'
            }
        }
    }
}
